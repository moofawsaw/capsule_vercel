{
  "redirects": [
    {
      "source": "/join/:type/:code",
      "destination": "https://capapp.co/join/:type/:code",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/:storyId", "destination": "/api/:storyId" }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ]
}

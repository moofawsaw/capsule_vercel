{
  "rewrites": [
    { "source": "/join/:type/:code", "destination": "/api/join/:type/:code" },
    { "source": "/:storyId", "destination": "/api/:storyId" }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ]
}
